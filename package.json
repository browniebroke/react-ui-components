{
  "name": "@browniebroke/react-ui-components",
  "version": "0.0.0",
  "description": "Collection of basic UI components.",
  "author": "browniebroke",
  "license": "MIT",
  "repository": "browniebroke/react-ui-components",
  "homepage": "https://github.com/browniebroke/react-ui-components#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-ui-components.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=8"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">= 5.0",
    "typography": ">=0.16"
  },
  "dependencies": {
    "color": "^3.2.0",
    "gray-percentage": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@size-limit/preset-small-lib": "7.0.8",
    "@storybook/addon-essentials": "6.4.18",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.4.18",
    "@storybook/addons": "6.4.18",
    "@storybook/react": "6.4.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.22",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "size-limit": "7.0.8",
    "styled-components": "5.3.3",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.5.5",
    "typography": "0.16.21"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "arrowParens": "always"
  },
  "size-limit": [
    {
      "path": "dist/react-ui-components.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/react-ui-components.esm.js",
      "limit": "15 KB"
    }
  ]
}
