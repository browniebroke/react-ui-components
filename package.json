{
  "name": "@browniebroke/react-ui-components",
  "version": "0.0.0",
  "description": "Collection of basic UI components.",
  "author": "browniebroke",
  "license": "MIT",
  "repository": "browniebroke/react-ui-components",
  "homepage": "https://github.com/browniebroke/react-ui-components#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-ui-components.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=8"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">= 5.0"
  },
  "dependencies": {
    "color": "4.2.3",
    "gray-percentage": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@size-limit/preset-small-lib": "8.1.0",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addons": "6.5.13",
    "@storybook/react": "6.5.13",
    "@types/react": "18.0.22",
    "@types/react-dom": "18.0.7",
    "@types/react-test-renderer": "18.0.0",
    "@types/styled-components": "5.1.26",
    "babel-jest": "29.2.2",
    "babel-loader": "8.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "size-limit": "8.1.0",
    "styled-components": "5.3.6",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "arrowParens": "always"
  },
  "size-limit": [
    {
      "path": "dist/react-ui-components.cjs.production.min.js",
      "limit": "15 KB"
    },
    {
      "path": "dist/react-ui-components.esm.js",
      "limit": "15 KB"
    }
  ],
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-app"
    ]
  }
}
